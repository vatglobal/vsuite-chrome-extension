{
   "name": "MyVsuite",
   "version": "1.0.0",
   "manifest_version": 2,
   "content_scripts": [
       {
           "matches": ["<all_urls>"],
           "js": ["content.js"]
       }
   ],
   "browser_action": {
       "default_popup": "popup.html",
       "default_title": "My Vsuite"
   },
   "icons": { 
    "16": "vSuite.png",
     "48": "vSuite.png",
     "128": "vSuite.png" 
 }
}